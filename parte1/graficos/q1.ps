set key off

set terminal png size 600, 400
set output "graficos/q1.10.png"

set title "Ajuste de y_{10}" font "Helvetica,20" norotate

set style line 1 lc rgb '#0060ad' lt 1 lw 2 pt 7 pi -1 ps 1.5

a0 = "`head data/question1.dat | awk '{print $1}'`"
a1 = "`head data/question1.dat | awk '{print $2}'`"
a2 = "`head data/question1.dat | awk '{print $3}'`"
a3 = "`head data/question1.dat | awk '{print $4}'`"
a4 = "`head data/question1.dat | awk '{print $5}'`"
a5 = "`head data/question1.dat | awk '{print $6}'`"
a6 = "`head data/question1.dat | awk '{print $7}'`"
a7 = "`head data/question1.dat | awk '{print $8}'`"
a8 = "`head data/question1.dat | awk '{print $9}'`"
a9 = "`head data/question1.dat | awk '{print $10}'`"
a10 = "`head data/question1.dat | awk '{print $11}'`"

plot a0 + a1*x + a2*x**2 + a3*x**3 + a4*x**4 + a5*x**5 + a6*x**6 + a7*x**7 + a8*x**8 + a9*x**9 + a10*x**10, "data/tabela1.dat" u 1:2 w p
